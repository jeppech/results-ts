{
  "name": "@jeppech/results-ts",
  "version": "1.2.1",
  "description": "Rust-like Result and Option types for TypeScript",
  "author": "Jeppe Christiansen",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeppech/results-ts.git"
  },
  "bugs": {
    "url": "https://github.com/jeppech/results-ts/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./utils": {
      "import": "./lib/utils.js",
      "types": "./lib/utils.d.ts"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepare": "pnpm build && pnpm clean && pnpm format && pnpm lint && pnpm build",
    "prepublishOnly": "pnpm lint",
    "format": "prettier --config .prettierrc --write 'src/**/*.ts'",
    "lint": "eslint src/",
    "clean": "rm -rf ./lib/**/*",
    "build": "tsc -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "result",
    "option",
    "rust",
    "typescript",
    "monad",
    "functional"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.8.7",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
